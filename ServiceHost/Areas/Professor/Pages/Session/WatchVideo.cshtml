@page
@model ServiceHost.Areas.Professor.Pages.Session.WatchVideoModel
@{
    
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>مشاهده ویدیو</title>
</head>
<body>

<style>
  .r1_iframe_embed {
    position: relative;
    overflow: hidden;
    width: 100%;
    height: auto;
    padding-top: 56.25%;
  }
  .r1_iframe_embed iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border: 0;
  }
</style>
<div class="r1_iframe_embed">
    <iframe
        src="@Model.VideoUrl"
        style="border: 0 #ffffff none"
        frameborder="0"
        allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
        allowfullscreen="true"
        webkitallowfullscreen="true"
        mozallowfullscreen="true"
    ></iframe>
</div>


</body>
</html>
